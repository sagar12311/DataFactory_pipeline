{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "P01-Dev"
		},
		"ADLS_LS1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_LS1'"
		},
		"SQL_DB_LS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SQL_DB_LS'"
		},
		"ADLS_LS1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://newprojectadls1998.dfs.core.windows.net/"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://P01-Dev-KeyV.vault.azure.net/"
		},
		"SQL_DB_LS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlsever-project1.database.windows.net"
		},
		"SQL_DB_LS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sagardemo"
		},
		"SQL_DB_LS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sagardemo"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "P01-Dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_LS1_accountKey": {
						"value": "[parameters('ADLS_LS1_accountKey')]"
					},
					"SQL_DB_LS_password": {
						"value": "[parameters('SQL_DB_LS_password')]"
					},
					"ADLS_LS1_properties_typeProperties_url": {
						"value": "[parameters('ADLS_LS1_properties_typeProperties_url')]"
					},
					"SQL_DB_LS_properties_typeProperties_server": {
						"value": "[parameters('SQL_DB_LS_properties_typeProperties_server')]"
					},
					"SQL_DB_LS_properties_typeProperties_database": {
						"value": "[parameters('SQL_DB_LS_properties_typeProperties_database')]"
					},
					"SQL_DB_LS_properties_typeProperties_userName": {
						"value": "[parameters('SQL_DB_LS_properties_typeProperties_userName')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}